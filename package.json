  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --write --unsafe"
    ],
    "*.{json}": [
      "biome format --write"
    ]
  }
